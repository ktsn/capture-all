{
  "name": "capture-block",
  "version": "0.0.0",
  "author": "katashin",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/ktsn/capture-block",
  "bugs": "https://github.com/ktsn/capture-block/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/capture-block.git"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "clean": "rm -rf lib",
    "build": "tsc -p src",
    "dev": "jest --watch",
    "lint": "tslint -p . && prettier --list-different \"{src,scripts,test}/**/*.{js,ts}\"",
    "format": "prettier --write \"{src,scripts,test}/**/*.{js,ts}\"",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/.+\\.spec\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "test/tsconfig.json"
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^22.1.4",
    "@types/puppeteer": "^1.0.1",
    "jest": "^22.4.2",
    "prettier": "1.11.0",
    "ts-jest": "^22.4.1",
    "tslint": "^5.9.1",
    "tslint-config-ktsn": "^2.1.0",
    "tslint-config-prettier": "^1.9.0",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "puppeteer": "^1.1.1"
  }
}
